{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends":["airbnb-base","prettier"],
    "plugins": ["prettier"],
    "globals": {
        "Atomics":"readonly",
        "SharedArrayBuffer":"readonly"
    },

    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier":"error"
    }
}
